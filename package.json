{
  "name": "mapbox-gl-codeflow-example",
  "version": "1.0.0",
  "description": "an example of using mapbox gl to style vector tiles by hand",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js --live -- -t babelify | garnish",
    "build": "cp index.html dist/ && browserify index.js > dist/index.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "keywords": [
    "mapbox",
    "gl",
    "codeflow",
    "example",
    "cli",
    "maps"
  ],
  "author": "Tom MacWright",
  "license": "ISC",
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^11.1.0",
    "budo": "^4.2.1",
    "chroma-js": "^1.1.1",
    "garnish": "^3.2.0",
    "mapbox-gl-style-spec": "^8.1.0"
  },
  "dependencies": {
    "mapbox-gl": "^0.11.0",
    "oam-browser-filters": "^2.0.0",
    "react": "^0.13.3"
  }
}
